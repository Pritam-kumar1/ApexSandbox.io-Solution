public boolean isSorted(List<Integer> numbers) {
    //code here
    if(numbers.isEmpty() || numbers.size()==1){
        return true;
    }if(numbers.size() >=2){
        for(Integer i=1;i<numbers.size();i++){
            if(numbers.get(i-1)<numbers.get(i))
            return false;
        }
        return true;
    }
    
    return true;
}