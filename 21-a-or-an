public String aOrAn(String word) {
    //code here
    Integer characterCode = word.toLowercase().charAt(0);
    String firstChar = String.fromCharArray(new Integer[]{ characterCode });
    if(firstChar == 'a' || firstChar == 'e' || firstChar == 'i' || firstChar == 'o' || firstChar == 'u'){
        return 'an '+ word;
    }
    return 'a '+ word;
}