public Integer nextPrime(Integer num) {
    //code here
    if(num >= 1 && num < 3)
    return num +1;
    for(Integer i = num ; i<2*n; i++ ){
        if(Math.Mod(i,2)==0 ||Math.Mod(i,3)==0)
        continue;
        for(Integer j= 5; j*j<i ; j=j+6){
            if(Math.Mod(i,j) ==0 || Math.Mod(i,j+2)== 0)
            continue;
        }
        return i;
    }
}