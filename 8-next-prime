public Integer nextPrime(Integer num) {
    //code here
    if(num >= 1 && num < 3)
    return num +1;
    for(Integer i = num ; i<2*n; i++ ){
        if(i%2==0 || i%3==0)
        continue;
        for(Integer j= 5; j*j<i ; j=j+6){
            if(i % j ==0 || i % (j+2) == 0)
            continue;
        }
        return i;
    }
}