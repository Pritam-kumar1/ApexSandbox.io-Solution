public class LinkedList{
    Node head;
    Node tail;
    Integer size;
    public void LinkedList(){
        this.head = null;
        this.tail = null;
        this.size = 0;
    }
    
    public void addToFront(Integer value){
        Node newNode = new Node();
        newNode.data = value;
        if(this.head == null){
            this.head = newNode;
            this.tail = this.head;
        }else{
            newNode.next = this.head;
            head = newNode;
        }
        this.size++;
    }
    public Integer removeFromFront(){
        if(head == null)
        return null;
        Integer value = head.data;
        head = head.next;
        size -- ;
        return value;
    }
    public void addToTail(Integer value){
        Node newNode = new Node();
        newNode.data = value;
        if(this.head == null){
            this.head = newNode;
            this.tail = this.head;
        }
        else{
            this.tail.next = newNode;
            tail = newNode;
        }
        size++;
    }
    public Integer removeFromTail(){
        if(size == 0)
        return null;
        if(size == 1){
            Integer data = head.data;
            head = null;
            size -- ;
            return data;
        }
        Node curr = head;
        Node newtail = null;
        while(curr.next !=null){
            newTail = curr;
            curr = curr.next;
        }
        Integer data = curr.data;
        tail = newTail;
        tail.next = null;
        size --;
        return data;
    }
    public Integer size(){
        return this.size;
    }    
}

public class Node{
    Integer data;
    Node next;
}