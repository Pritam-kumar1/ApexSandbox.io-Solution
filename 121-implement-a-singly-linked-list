public class LinkedList{
    Node head;
    Node tail;
    Integer size;

    public LinkedList(){
        this.head = null;
        this.tail = null;
        this.size = 0;
    }

    public void addToFront(Integer value){
        Node node = new Node();
        node.data = value;
        if (this.head == null) {
            this.head = node;
            this.tail = this.head;
        } else {
            node.next = this.head;
            this.head = node;
        }
        this.size++;
    }

    public Integer removeFromFront(){
        if(this.size == 0){
            return null;
        }
        Integer value = this.head.data;
        this.head = this.head.next;
        this.size--;
        return value;
    }

    public void addToTail(Integer value){
        Node newNode = new Node();
        newNode.data = value;
        if (this.head == null) {
            this.head = newNode;
            this.tail = this.head;
        }else {
            this.tail.next = newNode;
            this.tail = newNode;
        }
        this.size++;
    }

    public Integer removeFromTail(){
        if(size() == 0 ){
            return null;
        } else if(size() == 1){
            Integer data = head.data;
            head = null;
            size --;
            return data;
        } 
        Node current = head;
        Node newTail = null;
        while (current.next != null) {
            newTail = current;
            current = current.next;
        }
        Integer data = current.data;
        tail = newTail;
        tail.next = null; 
        size --;
        return data;
    }

    public Integer size(){
        return this.size;
    }
}

public class Node{
    Integer data;
    Node next;
}